<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		{{include('_menu.html.twig')}}

		<div class="container">
			<div class="row">
				{% block breadcrumb %}
					<nav aria-label="breadcrumb">
						<ol class="breadcrumb px-1">
							<li class="breadcrumb-item active" aria-current="page">
								<a href="/">Home</a>
							</li>
						</ol>
					</nav>
				{% endblock %}
			</div>
			<div class="row">
				<div class="col-12 col-md-9"> {% block body %}{% endblock %}
					</div>
					<div class="col-12 col-md-3">{{include('_aside.html.twig')}}</div>
				</div>
			</div>
		</body>
	</html>
